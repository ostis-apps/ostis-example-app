sheaf
<- sc_node_class;
=> nrel_main_idtf:
	[связь]
	(* <- lang_ru;; *);
	[sheaf]
	(* <- lang_en;; *);
=> nrel_idtf:
	[связка sc-элементов]
	(* <- lang_ru;; *);
=> nrel_idtf:
	[sc-связка]
	(* <- lang_ru;; *);	
<= nrel_subdividing:
	{
		binary_sheaf;
		non_binary_sheaf
	};
<= nrel_subdividing:
	{
		oriented_sheaf;
		non_oriented_sheaf
	};;

